record(bo,      "$(P):llrfAmcInit") {
    field(DESC, "Run llrfAmc initialization sequence")
    field(DTYP, "asynUInt32Digital")
    field(SCAN, "Passive")
    field(OUT,  "@asynMask($(PORT),0x01)INIT")
}

record(mbbi,    "$(P):llrfAmcInitStatus") {
    field(DESC, "llrfAmc initialization status")
    field(DTYP, "asynUInt32Digital")
    field(SCAN, "I/O Intr")
    field(INP,  "@asynMask($(PORT),0x03)INIT_STAT")
    field(NOBT, "2")
    field(ZRVL, "0")
    field(ONVL, "1")
    field(TWVL, "2")
    field(ZRST, "Failed")
    field(ONST, "Succeed")
    field(TWST, "In progress...")
    field(PINI, "YES")
}